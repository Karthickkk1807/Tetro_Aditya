                            <form id="FromTransactionsInfo">
                                <div class="row">
                                    <div class="col-12" id="TransactionsInfo">
                                        <div class="table-responsive">
                                            <table class="table table-rounded dataTable data-table table-striped tableResponsive" id="Managetable"></table>
