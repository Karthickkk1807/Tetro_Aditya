<div class="row justify-content-end mr-2">
    <div class="ml-3 mr-3">
        <label class="mb-0">
            Show
            <select class="customLengthDropdown custom-select custom-select-sm form-control d-inline-block ml-1"
                    data-table="#ServiceEngrTable" style="width: 69px;">

                <option value="10">10</option>
                <option value="25">25</option>
                <option value="50">50</option>
                <option value="100">100</option>
            </select> entries
        </label>
    </div>
    <div class="searchbar">
        <input type="text" class="searchbar__input" name="q" id="tableFilter" placeholder="Search">
        <button type="submit" class="searchbar__button">
            <img src="~/assets/commonimages/search.svg" />
        </button>
    </div>
    <div class="datapiker ml-2 mt-1" style="position: relative;">
        <div class="add-imp-exp-btn">
            <div class="exportsiconpop">
                @{
                    if (access.Create)
                    {
                        <span class="pop">
                            <a id="AddClient" title="Add ServiceEngr">
                                <img src="~/assets/CommonImages/addicon.svg" style="height: 25px; width: 25px;" />
                            </a>
                        </span>
                    }
                }
            </div>
        </div>
    </div>
</div>

<div class="table-responsive">
    <table class="table table-rounded dataTable data-table table-striped tableResponsive table-bordered" id="ServiceEngrTable"></table>
</div>

<div id="ClientCanvas" class="offcanvas-container">
    <div class="offcanvas-header d-flex align-items-center justify-content-between">
        <h2 id="ClientHeader">Add New Client</h2>
        <span id="CloseCanvas" class="close"
              style="font-size:30px;cursor:pointer;color:white;"
              title="Close">&times;</span>
    </div>
    <div class="offcanvas-body">
        <div class="row">
            <div class="col-12">
                <div class="accordion" id="">
                    <div class="accordion-header">
                        <h2 id="Styleentery" class="mb-0 Styleentery">
                            <a data-toggle="collapse" data-target="#collapse1" aria-expanded="true" aria-controls="collapseOne1" class="">
                                <p class="d-flex align-items-center justify-content-between mb-0 px-3 py-2">
                                    <strong class="text-white">
                                        Service Engr Info
                                    </strong>
                                    <i class="fas fa-angle-down text-white"></i>
                                </p>
                            </a>
                        </h2>
                    </div>
                    <div id="collapse1" class="collapse show" aria-labelledby="headingOne" data-parent="#styleAccordion1" style="">
                        <div id="Styleenterydetalis" class="card-body">
                            <form id="FormServiceEngr">
                                <div class="row">
                                    <div class="col-md-6 col-lg-6 col-sm-6 col-6">
                                        <div class="form-group">
                                            <label>ServiceEngr Id<span id="Asterisk">*</span></label>
                                            <input type="text" class="form-control" placeholder="Ex: SER_ENGR_2025" id="ServiceEngrNo" name="ServiceEngrNo" required disabled />
                                        </div>
                                    </div>
                                    <div class="col-md-6 col-lg-6 col-sm-6 col-6">
                                        <div class="form-group">
                                            <label>ServiceEngr Type<span id="Asterisk">*</span></label>
                                            <select class="form-control" id="ServiceEngrTypeId" name="ServiceEngrTypeId" required>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="col-md-6 col-lg-6 col-sm-6 col-6" id="CompanyNameDiv">
                                        <div class="form-group">
                                            <label id="ClientTypeName">Company Name<span id="Asterisk">*</span></label>
                                            <input type="text" class="form-control" placeholder="Ex: Anegan DS" id="ServiceEngrName" name="ServiceEngrName" maxlength="50" required />
                                        </div>
                                    </div>
                                    <div class="col-md-6 col-lg-6 col-sm-6 col-6">
                                        <div class="form-group">
                                            <label>Address</label>
                                            <input type="text" class="form-control" placeholder="Ex: 101, Ashoka Nagar" id="Address" name="Address" maxlength="500" />
                                        </div>
                                    </div>
                                    <div class="col-md-6 col-lg-6 col-sm-6 col-6">
                                        <div class="form-group">
                                            <label>City<span id="Asterisk">*</span></label>
                                            <input type="text" class="form-control" placeholder="Ex: Chennai" id="City" name="City" required oninput="Common.allowOnlyTextLength(this,50)" />
                                        </div>
                                    </div>
                                    <div class="col-md-6 col-lg-6 col-sm-6 col-6 mt-2">
                                        <div class="form-group">
                                            <label>State</label>
                                            <input type="text" class="form-control" placeholder="Ex: TamilNadu" id="State" name="State" required oninput="Common.allowOnlyTextLength(this,50)" />
                                        </div>
                                    </div>
                                    <div class="col-md-6 col-lg-6 col-sm-6 col-6 mt-2">
                                        <div class="form-group">
                                            <label>Country</label>
                                            <input type="text" class="form-control" placeholder="Ex: India" id="Country" name="Country" oninput="Common.allowOnlyTextLength(this,20)" required />
                                        </div>
                                    </div>

                                    <div class="col-md-6 col-lg-6 col-sm-6 col-6 mt-2">
                                        <div class="form-group">
                                            <label>Zipcode</label>
                                            <input type="text" class="form-control" placeholder="Ex: 600028" id="ZipCode" name="ZipCode" oninput="Common.allowOnlyNumberLength(this,6)" />
                                        </div>
                                    </div>

                                    <div class="col-md-6 col-lg-6 col-sm-6 col-6 mt-2">
                                        <div class="form-group">
                                            <label>Contact No<span id="Asterisk">*</span></label>
                                            <input type="text" class="form-control" placeholder="Ex:9876543210" id="ContactNumber" name="ContactNumber" minlength="6" oninput="Common.allowOnlyContactNoAlternativeLength(this,20)" required />
                                        </div>
                                    </div>

                                    <div class="col-md-6 col-lg-6 col-sm-6 col-6 mt-2">
                                        <div class="form-group">
                                            <label>What'sApp No<span id="Asterisk">*</span></label>
                                            <input type="text" class="form-control" placeholder="Ex:9876543210" id="WhatsappNo" name="WhatsappNo" minlength="6" oninput="Common.allowOnlyContactNoAlternativeLength(this,20)" required />
                                        </div>
                                    </div>
                                    <div class="col-md-6 col-lg-6 col-sm-6 col-6 mt-2">
                                        <div class="form-group">
                                            <label>Email</label>
                                            <input type="text" class="form-control" placeholder="Ex: example@gmail.com" id="Email" name="Email" oninput="Common.allowEmailAndLength(this, 50)" />
                                        </div>
                                    </div>

                                    <div class="col-md-6 col-lg-6 col-sm-6 col-6 mt-2" id="GSTNone">
                                        <div class="form-group">
                                            <label>GST Number</label>
                                            <input type="text" class="form-control" placeholder="Ex: 27AAACB1234C1Z5" id="GSTNumber" name="GSTNumber" oninput="Common.allowGSTNumber(this, 15)" />
                                        </div>
                                    </div>
                                    @*
                                    <div class="col-md-6 col-lg-6 col-sm-6 col-6 mt-2">
                                    <div class="form-group">
                                    <label>Max Credit Limit<span id="Asterisk">*</span></label>
                                    <input type="text" class="form-control" placeholder="Max Credit Limit" id="CreditLimit" name="CreditLimit" oninput="Common.allowOnlyNumberLength(this,8)" required />
                                    </div>
                                    </div> *@
                                    <div class="col-md-6 col-lg-6 col-sm-6 col-6 mt-2" id="CurrentlimitHide">
                                        <div class="form-group">
                                            <label>Current Credit</label>
                                            <input type="text" class="form-control" placeholder="0.00" id="CurrentCreditLimit" name="CurrentCreditLimit" oninput="Common.allowOnlyNumbersAndDecimalwithmaxlength(this, 6)" />
                                        </div>
                                    </div>

                                    <div class="col-md-3 col-lg-3 col-sm-3 col-6" style="margin-top: 35px;" id="IsActiveHide">
                                        <div class="form-group">
                                            <input type="checkbox" id="IsActive" name="IsActive">
                                            <label for="IsActive"> Is Active </label>
                                        </div>
                                    </div>


                                    <div class="col-md-12 col-lg-12 col-sm-12 col-12 mt-2">
                                        <div class="form-group">
                                            <label>Remarks</label>
                                            <textarea class="form-control" id="Remarks" autocomplete="off" name="Remarks" rows="3" maxlength="500" placeholder="Remarks"></textarea>
                                        </div>
                                    </div>

                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>

            <div class="col-12">
                <div class="accordion" id="">
                    <div class="accordion-header">
                        <h2 id="Styleentery" class="mb-0 Styleentery">
                            <a data-toggle="collapse" data-target="#collapse2" aria-expanded="true" aria-controls="collapseOne1" class="">
                                <p class="d-flex align-items-center justify-content-between mb-0 px-3 py-2">
                                    <strong class="text-white">
                                        Contact Person Info
                                    </strong>
                                    <i class="fas fa-angle-down text-white"></i>
                                </p>
                            </a>
                        </h2>
                    </div>
                    <div id="collapse2" class="collapse" aria-labelledby="headingOne" data-parent="#styleAccordion1" style="">
                        <div id="Styleenterydetalis" class="card-body">

                            <div class="d-flex justify-content-end align-items-start">
                                <button id="" class="btn AddStockBtn" type="button" onclick="duplicateRow(this)">
                                    <i class="fas fa-plus" id="AddButton"></i>
                                </button>
                            </div>
                            <form id="FormContactService">
                            </form>
                        </div>
                    </div>
                </div>
            </div>

