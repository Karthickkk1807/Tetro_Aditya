@using TetroONE.Models;
@using TetroONE;
@{
    UserAccessOption access = GenericTetroONE.GetUserAccessSession(UserScreen.Distributor, Context.Session);
}
<link href="~/css/ServiceENGR.css" rel="stylesheet" />
<style>
    table th {
        font-size: 12px;
        font-weight: 800;
    }

    .Managetable table > thead > tr > th:first-child {
        border-top-left-radius: 10px;
    }

    .Managetable table > thead > tr > th:last-child {
        border-top-right-radius: 10px;
    }

    .Managetable tr:last-child td:first-child {
        border-bottom-left-radius: 10px;
    }

    .Managetable tr:last-child td:last-child {
        border-bottom-right-radius: 10px;
    }

    .badge {
        padding: 5px 10px;
        border-radius: 4px;
        color: white;
        font-weight: bold;
        display: inline-block;
        width: 100%;
        text-align: center;
    }

    .in_sent {
        background-color: #28a745;
    }

    .in_draft {
        background-color: #6c757d;
    }
</style>

